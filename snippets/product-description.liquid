<div class="sin-pro-fea-pro-sec">
	<div class="sin-pro-fea-pro-inn">
		<div class="sin-pro-fea-pro-row">
			<div class="sin-pro-fea-pro-left">
				<div class="sin-pro-fea-pro-img"><img src="{{section.settings.main_banner| img_url: "original"}}" /></div>
			</div>
			<div class="sin-pro-fea-pro-right">
				<div class="sin-pro-fea-pro-cont">
					<h3>{{section.settings.heading}}</h3>
					<ul>
						<li>
                          <div>
                           {{section.settings.icon_1}}
                            </div>
							<h4>{{section.settings.title_1}}</h4>
							<p>{{section.settings.text_1}}</p>
						</li>
						<li>
                          <div>
                           {{section.settings.icon_2}}
                            </div>
							<h4>{{section.settings.title_2}}</h4>
							<p>{{section.settings.text_2}}</p>
						</li>
						<li>
                          <div>
                           {{section.settings.icon_3}}
                            </div>
							<h4>{{section.settings.title_3}}</h4>
							<p>{{section.settings.text_3}}</p>
						</li>
						<li>
                          <div>
                           {{section.settings.icon_4}}
                            </div>
							<h4>{{section.settings.title_4}}</h4>
							<p>{{section.settings.text_4}}</p>
						</li>
					</ul>
				</div>
			</div>
		</div>
	</div>
</div>
<div class="sin-pro-img-cont-lr-main">
	<div class="sin-pro-img-cont-lr-inn">
     {% for block in section.blocks %}
      {% assign getnumber = forloop.index | modulo:2 %}
		<div class="sin-pro-img-cont-lr-row">
			<div class="sin-pro-img-cont-lr-left">
				<div class="sin-pro-img-cont-lr-img"><img src="{{ block.settings.image | img_url:'original'}}" /></div>
			</div>
			<div class="sin-pro-img-cont-lr-right">
				<div class="sin-pro-img-cont-lr-cont">
					<h4>{{ block.settings.title}}</h4>
					<h2>{{ block.settings.heading}}</h2>
					<p>{{ block.settings.text}}</p>
				</div>
			</div>
		</div>
      {%endfor%}				
	</div>
</div>