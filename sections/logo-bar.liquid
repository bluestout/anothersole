<div class="hm-fea-icon-txt-main">
	<div class="hm-fea-icon-txt-inn">
      <div class="hm-fea-mobile-heading sec-hide-for-desktop">{{section.settings.heading}}</div>
		<div class="hm-fea-icon-txt-row">
        
           {% for block in section.blocks %}
			<div class="hm-fea-icon-txt-block">
				<div class="hm-fea-icon-txt-box">
					<div class="hm-fea-icon-txt-box-img"><img src=" {{ block.settings.image | img_url:'medium'}}" /></div>
					<p>{{block.settings.title}}</p>
				</div>
			</div>
          {%endfor%}
		</div>
	</div>
</div>

