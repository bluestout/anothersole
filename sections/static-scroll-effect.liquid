<div class="hm-lt-sticky-section-main">
	<div class="hm-lt-sticky-section-inn">
		<div class="hm-lt-sticky-section-row">
          <div class="hm-lt-sticky-section-right">
            
				<ul>
                  {% for block in section.blocks %}
					<li>
						<a href="{{ block.settings.link}}">
							<img class="hm-lt-sticky-section-img-def lazyload" data-src="{{ block.settings.image | img_url: 'large'}}" />
                            {% comment %} <img class="hm-lt-sticky-section-img-hov lazyload" data-src="{{ block.settings.hover_image | img_url: 'large'}}" /> {% endcomment %}
                            <img class="hm-lt-sticky-section-img-hov" src="{{ block.settings.hover_image | img_url: 'large'}}" />
							<div class="hm-lt-sticky-section-right-cont">
								<h3>{{ block.settings.title}}</h3>
								<p>{{ block.settings.sub_title}}</p>
							</div>
						</a>
					</li>
                  {%endfor%}
					
				</ul>
			</div>
			<div class="hm-lt-sticky-section-left">
        
				<div class="hm-lt-sticky-section-left-slider cst-slider-dots-lt">
        
					<div class="hm-lt-sticky-section-left-cont">
						<h3>{{section.settings.heading_1}}</h3>
						<p>{{section.settings.sub_heading_1}}</p>
					</div>
                  <div class="hm-lt-sticky-section-left-cont">
					<h3>{{section.settings.heading_2}}</h3>
						<p>{{section.settings.sub_heading_2}}</p>
					</div>
                  <div class="hm-lt-sticky-section-left-cont">
						<h3>{{section.settings.heading_3}}</h3>
						<p>{{section.settings.sub_heading_3}}</p>
					</div>
				</div>
                              <div class="hm-lt-sticky-section-left-btn">
	<a href="{{section.settings.btn_link}}" class="blk-bk-rounded-btn blk-bk-right-arw-btn">{{section.settings.btn_text}}</a>
</div>
			</div>
			
		</div>
	</div>
</div>
