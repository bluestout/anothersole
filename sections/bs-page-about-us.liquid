      <h2>Our Mission</h2>
      <h3>#buy1feed1</h3>
      <p>
        Its not enough that we are producing great products at great prices,
        <br>we also need to be contributing to helping a cause that is close to our hearts. 
      </p>
      <span>Join us on this endless path of giving back to hungry children.</span>
      <a class="bs-about-mk-btn" href="{{section.settings.mission_link}}">LEARN MORE</a>
    </div>
    <div class="bs-about-miss-right">
      {%- assign img_url = section.settings.miss_img | img_url: '2x2' | replace: '_2x2.', '_{width}x.' -%}
        <img class="lazyload js"
            data-src="{{ img_url }}"
            data-widths="[180, 360, 540, 720, 900, 1080, 1296, 1512, 1728, 2048]"
            data-sizes="auto"
            />
    </div>
  </div>
</div>


{% schema %}
{
  "name": "About Us",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "title",
      "default": "Title"
    },
    {
      "type": "text",
      "id": "description",
      "label": "description",
      "default": "description"
    },
    {
      "type": "image_picker",
      "id": "about2_img",
      "label": "About2 Image",
      "info": "Recommend 1064 x 1080px"
    },
    {
      "type": "image_picker",
      "id": "about2_img_mob",
      "label": "About2 Image Mobile",
      "info": "Recommend 1500 x 1228px"
    },
    {
      "type": "image_picker",
      "id": "diff_img",
      "label": "Diff Image",
      "info": "Recommend 1050 x 1050px"
    },
    {
      "type": "image_picker",
      "id": "miss_img",
      "label": "Mission Image",
      "info": "Recommend 1050 x 1050px"
    },
    {
      "type": "url",
      "id": "mission_link",
      "label": "Mission link"
    }
  ]
}
{% endschema %}
