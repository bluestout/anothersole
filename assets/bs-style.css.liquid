  position: absolute;
  right: 0px;
  top: 25px;
}
.bs-page-faq-sec .ui-accordion-header .ui-accordion-header-icon:before{
  content:"";
  width: 11px;
  height: 2px;
  position: absolute;
  left: 0;
  top: 5px;
  background-color: #4c4c4c;
}
